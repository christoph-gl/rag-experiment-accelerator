# For more information on environment variables, please refer to the documentation in ./docs/environment-variables.md

#### Azure Search Service
AZURE_SEARCH_SERVICE_ENDPOINT=
AZURE_SEARCH_ADMIN_KEY=

#### OpenAI
OPENAI_API_KEY=
# Must be 'azure' or 'open_ai'
OPENAI_API_TYPE=

##### Azure OpenAI
# Required when OPENAI_API_TYPE is set to 'azure'
OPENAI_ENDPOINT=
OPENAI_API_VERSION=

#### Azure Machine Learning
AML_SUBSCRIPTION_ID=
AML_WORKSPACE_NAME=
AML_RESOURCE_GROUP_NAME=

#### Azure Document Intelligence
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=
AZURE_DOCUMENT_INTELLIGENCE_ADMIN_KEY=

############
# OPTIONAL
############

#### Azure Search Skillsets
AZURE_LANGUAGE_SERVICE_ENDPOINT=
AZURE_LANGUAGE_SERVICE_KEY=

# One of: NOTSET, DEBUG, INFO, WARN, ERROR, CRITICAL. Default is INFO
# LOGGING_LEVEL=

#### Virtual Network
# Required when deploying the application in network isolation
# Example: VIRTUAL_NETWORK_ADDRESS_SPACE=10.0.0.0/16
# Example: PROXY_SERVER_SUBNET_ADDRESS_SPACE=10.0.1.0/24
# Example: AZURE_RESOURCES_SUBNET_ADDRESS_SPACE=10.0.2.0/24
# Replace these example values with your actual virtual network configuration
VIRTUAL_NETWORK_ADDRESS_SPACE=
PROXY_SERVER_SUBNET_ADDRESS_SPACE=
AZURE_RESOURCES_SUBNET_ADDRESS_SPACE=
